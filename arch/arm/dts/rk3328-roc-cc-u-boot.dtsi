#include "rk3328-u-boot.dtsi"
#include "rk3328-sdram-ddr4-1600.dtsi"
/ {
	chosen {
		u-boot,spl-boot-order = "same-as-spl", &emmc, &sdmmc;
	};

	smbios {
		compatible = "u-boot,sysinfo-smbios";

		smbios {
			system {
				manufacturer = "libre-computer";
				product = "roc-rk3328-cc";
			};

			baseboard {
				manufacturer = "libre-computer";
				product = "roc-rk3328-cc";
			};

			chassis {
				manufacturer = "libre-computer";
				product = "roc-rk3328-cc";
			};
		};
	};
};

&gpio0 {
	bootph-pre-ram;
};

&pinctrl {
	bootph-pre-ram;
};

&sdmmc0m1_pin {
	bootph-pre-ram;
};

&pcfg_pull_up_4ma {
	bootph-pre-ram;
};

/* Need this and all the pinctrl/gpio stuff above to set pinmux */
&vcc_sd {
	bootph-pre-ram;
};
