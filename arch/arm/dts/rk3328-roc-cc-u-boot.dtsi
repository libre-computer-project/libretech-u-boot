#include "rk3328-u-boot.dtsi"
#include "rk3328-sdram-ddr4-1600.dtsi"

/ {
	smbios {
		compatible = "u-boot,sysinfo-smbios";

		smbios {
			system {
				manufacturer = "firefly";
				product = "roc-rk3328-cc";
			};

			baseboard {
				manufacturer = "firefly";
				product = "roc-rk3328-cc";
			};

			chassis {
				manufacturer = "firefly";
				product = "roc-rk3328-cc";
			};
		};
	};
};

&usb20_otg {
	dr_mode = "peripheral";
};

&vcc_sd {
	bootph-pre-ram;
};
